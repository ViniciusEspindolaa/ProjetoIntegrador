{
  "name": "aula2",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npx ts-node-dev --respawn --clear index.ts",
    "start": "npx ts-node index.ts",
    "start:prod": "node dist/index.js",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:prod": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:seed": "npx ts-node prisma/seed.ts",
    "logs:clean": "rm -rf logs/*.log",
    "setup": "npm install && npx prisma generate && cp .env.example .env",
    "postinstall": "npx prisma generate",
    "deploy:dev": "bash deploy.sh development",
    "deploy:prod": "bash deploy.sh producao",
    "docker:build": "docker build -t petfinder-api .",
    "docker:run": "docker run -p 3001:3001 --env-file .env petfinder-api",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop petfinder-api",
    "pm2:restart": "pm2 restart petfinder-api",
    "pm2:logs": "pm2 logs petfinder-api",
    "pm2:monit": "pm2 monit",
    "worker:notificacoes": "npx ts-node ./scripts/worker-notificacoes.ts"
  },
